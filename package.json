{
  "name": "@cmfcmf/docusaurus-search-local",
  "version": "0.0.5",
  "description": "Local Search for Docusaurus",
  "keywords": [
    "docusaurus",
    "search",
    "plugin"
  ],
  "license": "MIT",
  "repository": "cmfcmf/docusaurus-search-local",
  "main": "src/index.js",
  "publishConfig": {
    "access": "public"
  },
  "author": "Christian Flach",
  "dependencies": {
    "autocomplete.js": "^0.37.0",
    "classnames": "^2.2.6",
    "htmlparser2": "^4.0.0",
    "lunr": "^2.3.8"
  },
  "peerDependencies": {
    "@docusaurus/core": "^2.0.0"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "lint": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
